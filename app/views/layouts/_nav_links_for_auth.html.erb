<% if doctor_signed_in? %>
  <%= link_to 'Edit Account', edit_doctor_registration_path, class: 'navbar-brand' %>
  <%= link_to 'Your Details', edit_doctor_path(current_doctor.id), class: 'navbar-brand' %>
  <%= link_to 'Sign out', destroy_doctor_session_path, :method=>'delete', class: 'navbar-brand' %>
<% elsif user_signed_in? %>
  <%= link_to 'Doctor Categories', doctor_categories_path, class: 'navbar-brand' %>
  <%= link_to 'Doctors', doctors_path, class: 'navbar-brand' %>
  <%= link_to 'Admins', users_path, class: 'navbar-brand' %>
  <%= link_to 'Edit Account', edit_user_registration_path, class: 'navbar-brand' %>
  <%= link_to 'Sign out', destroy_user_session_path, :method=>'delete', class: 'navbar-brand' %>
<% elsif patient_signed_in? %>
  <%= link_to 'Edit Account', edit_patient_registration_path, class: 'navbar-brand' %>
  <%#= link_to 'Your Details', edit_patient_path(current_patient.id), class: 'navbar-brand' %>
  <%= link_to 'Sign out', destroy_patient_session_path, :method=>'delete', class: 'navbar-brand' %>
<% else %>
  <%#= link_to 'Admin Sign in', new_user_session_path, class: 'navbar-brand' %>
  <%#= link_to 'Admin Sign up', new_user_registration_path, class: 'navbar-brand' %>
  <%= link_to 'Doctor Sign in', new_doctor_session_path, class: 'navbar-brand' %>
  <%= link_to 'Doctor Sign up', new_doctor_registration_path, class: 'navbar-brand' %>
  <%= link_to 'Patient Sign in', new_patient_session_path, class: 'navbar-brand' %>
  <%= link_to 'Patient Sign up', new_patient_registration_path, class: 'navbar-brand' %>
<% end %>


